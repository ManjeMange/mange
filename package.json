{
  "name": "mange",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "ci": "yarn lint && yarn scheck",
    "format": "prettier --write \"./src/**/*.{js,svelte,html,ts}\"",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "lint:fix": "yarn lint --fix",
    "prelint:fix": "yarn format",
